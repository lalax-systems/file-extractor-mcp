{
  "name": "@lalax-systems/file-extractor-mcp",
  "version": "0.1.0",
  "description": "MCP server para extraer, organizar y gestionar archivos entre directorios",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "file-extractor-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "dev": "tsx watch src/index.ts",
    "start": "node build/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["mcp", "file", "extractor", "organizer", "filesystem", "lalax-systems"],
  "author": "Javier Gomez <javier@lalax.systems>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lalax-systems/file-extractor-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/lalax-systems/file-extractor-mcp/issues"
  },
  "homepage": "https://github.com/lalax-systems/file-extractor-mcp#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}